t148 := #0
IF start >= end GOTO label22
GOTO label23
LABEL label22 :
t148 := #1
LABEL label23 :
t156 := end - start
t157 := #2
t155 := t156 / t157
t153 := start + t155
mid := t153
ARG mid
ARG start
